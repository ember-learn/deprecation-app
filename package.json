  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16",
    "npm": "8"
  },
  "ember-addon": {
    "paths": [
      "lib/content-docs-generator"
    ]
  },
  "fastbootDependencies": [
    "crypto"
  ]
}
