{
  "name": "deprecation-app",
  "version": "1.0.0",
  "private": true,
  "description": "The app that powers emberjs.com deprecation pages",
  "repository": "https://github.com/ember-learn/deprecation-app.git",
  "license": "MIT",
  "author": "Ember Learning Team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:frontmatter": "mocha --reporter ./lib/mocha-quiet.cjs node-tests",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint --ignore-pattern ./content/**/*.md . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:md": "remark . --frail --quiet",
    "lint:markdownlint": "markdownlint \"content/**/*.md\" --config .markdownlint.json",
    "start": "ember serve",
    "test": "concurrently \"pnpm:test:*\" --names \"test:\" --prefixColors auto",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@ember/legacy-built-in-components": "^0.4.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.7.0",
    "@embroider/macros": "^1.19.7",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@percy/cli": "^1.25.0",
    "@percy/ember": "^4.2.0",
    "algoliasearch": "^3.35.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-static-site-json": "^4.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-auto-import": "^2.6.0",
    "ember-cli": "~4.10.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-cjs-transform": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-prember-algolia": "^1.0.1",
    "ember-cli-fastboot": "^4.1.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-showdown": "^7.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.1.2",
    "ember-concurrency-decorators": "^2.0.3",
    "ember-data": "~4.4.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.2",
    "ember-href-to": "^5.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-set-body-class": "^1.0.2",
    "ember-showdown-shiki": "^1.2.1",
    "ember-source": "~4.4.0",
    "ember-styleguide": "^11.0.3",
    "ember-template-lint": "^5.7.1",
    "ember-test-selectors": "^6.0.0",
    "ember-tether": "^2.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-qunit": "^8.2.6",
    "globals": "^17.3.0",
    "loader.js": "^4.7.0",
    "markdownlint": "^0.34.0",
    "markdownlint-cli": "^0.41.0",
    "mocha": "^10.2.0",
    "prember": "^2.0.0",
    "prettier": "^3.8.0",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.1",
    "sass": "^1.58.0",
    "semver": "^7.7.4",
    "semver-compare": "^1.0.0",
    "walk-sync": "^3.0.0",
    "webpack": "^5.89.0",
    "yaml-front-matter": "^4.1.1"
  },
  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember-addon": {
    "paths": [
      "lib/content-docs-generator"
    ]
  },
  "fastbootDependencies": [
    "crypto"
  ],
  "packageManager": "pnpm@9.5.0"
}
